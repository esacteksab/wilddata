{
  "extends": [
    "config:base",
    "group:docker/*",
    "helpers:pinGitHubActionDigests",
    ":dependencyDashboard"
  ],
  "packageRules": [
    {
      "rebaseWhen": "behind-base-branch",
      "matchUpdateTypes": ["patch", "minor", "pin", "digest"],
      "automerge": true,
      "lockFileMaintenance": { "enabled": true },
      "labels": ["dependencies"]
    }

  ]

}
